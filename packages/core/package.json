{
  "name": "@preset/core",
  "version": "0.5.4",
  "license": "MIT",
  "author": "Enzo Innocenzi",
  "description": "Elegant, ecosystem-agnostic preset mechanism",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preset/preset.git",
    "directory": "packages/core"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*",
    "./globals": {
      "types": "./globals.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bugs": "https://github.com/preset/preset/issues",
  "homepage": "https://preset.dev",
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --external esbuild",
    "test": "vitest",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "prepublish": "esno ./update-globals.ts",
    "postpublish": "esno ./update-globals.ts --delete"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "type-fest": "^2.11.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "debug": "^4.3.3",
    "deepmerge": "^4.2.2",
    "detect-indent": "^7.0.0",
    "esbuild": "^0.14.16",
    "execa": "^6.0.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0",
    "mitt": "^3.0.0",
    "simple-git": "^3.1.1",
    "unconfig": "^0.2.2",
    "unset-value": "^2.0.0"
  }
}

{
	"name": "@preset/core",
	"type": "module",
	"version": "0.11.3",
	"description": "Elegant, ecosystem-agnostic preset mechanism",
	"author": "Enzo Innocenzi",
	"license": "MIT",
	"homepage": "https://preset.dev",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/preset/preset.git",
		"directory": "packages/core"
	},
	"publishConfig": {
		"access": "public"
	},
	"bugs": "https://github.com/preset/preset/issues",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.mts"
		},
		"./*": "./*",
		"./globals": {
			"types": "./globals.d.ts"
		}
	},
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"files": [
		"*.d.ts",
		"dist"
	],
	"scripts": {
		"build": "obuild",
		"stub": "obuild --stub",
		"test": "vitest",
		"test:watch": "vitest",
		"prepublishOnly": "npm run build",
		"prepublish": "tsx ./update-globals.ts",
		"postpublish": "tsx ./update-globals.ts --delete"
	},
	"dependencies": {
		"cac": "catalog:",
		"debug": "catalog:",
		"deepmerge": "catalog:",
		"esbuild": "catalog:",
		"fast-glob": "catalog:",
		"fs-extra": "catalog:",
		"mitt": "catalog:",
		"simple-git": "catalog:",
		"type-fest": "catalog:",
		"unconfig": "catalog:",
		"unset-value": "catalog:"
	},
	"devDependencies": {
		"@types/debug": "catalog:",
		"@types/fs-extra": "catalog:",
		"detect-indent": "catalog:",
		"execa": "catalog:",
		"typescript": "catalog:"
	}
}
